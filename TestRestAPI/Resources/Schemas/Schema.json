{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/WorkTool",
  "definitions": {
    "WorkTool": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "result": {
          "$ref": "#/definitions/Result"
        },
        "generatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "null"
        }
      },
      "required": [
        "generatedAt",
        "message",
        "result",
        "success"
      ],
      "title": "WorkTool"
    },
    "Result": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "maker": {
          "type": "null"
        },
        "model": {
          "type": "null"
        },
        "serialNumber": {
          "type": "string"
        },
        "workToolType": {
          "$ref": "#/definitions/ExternalPartner"
        },
        "externalPartner": {
          "$ref": "#/definitions/ExternalPartner"
        },
        "modifiedDate": {
          "type": "string",
          "format": "date-time"
        },
        "ceMarked": {
          "type": "null"
        },
        "documents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Document"
          }
        },
        "maximumCapacity": {
          "type": "null"
        },
        "manufacturerYear": {
          "type": "null"
        },
        "isActive": {
          "type": "boolean"
        },
        "csoApprovalDate": {
          "type": "null"
        },
        "isAccessControlApprovalSubmited": {
          "type": "boolean"
        },
        "comment": {
          "type": "null"
        },
        "createdByExternalPartner": {
          "$ref": "#/definitions/CreatedByExternalPartner"
        },
        "id": {
          "type": "integer"
        },
        "siteId": {
          "type": "integer"
        }
      },
      "required": [
        "ceMarked",
        "comment",
        "createdByExternalPartner",
        "csoApprovalDate",
        "documents",
        "externalPartner",
        "id",
        "isAccessControlApprovalSubmited",
        "isActive",
        "maker",
        "manufacturerYear",
        "maximumCapacity",
        "model",
        "modifiedDate",
        "name",
        "serialNumber",
        "siteId",
        "workToolType"
      ],
      "title": "Result"
    },
    "CreatedByExternalPartner": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "siteId": {
          "type": "integer"
        }
      },
      "required": [
        "id",
        "siteId"
      ],
      "title": "CreatedByExternalPartner"
    },
    "Document": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "documentType": {
          "$ref": "#/definitions/ExternalPartner"
        },
        "name": {
          "type": "null"
        },
        "description": {
          "type": "null"
        },
        "expiredDate": {
          "type": "null"
        },
        "id": {
          "type": "integer"
        },
        "siteId": {
          "type": "integer"
        }
      },
      "required": [
        "description",
        "documentType",
        "expiredDate",
        "id",
        "name",
        "siteId"
      ],
      "title": "Document"
    },
    "ExternalPartner": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        },
        "siteId": {
          "type": "integer"
        }
      },
      "required": [
        "id",
        "name",
        "siteId"
      ],
      "title": "ExternalPartner"
    }
  }
}
